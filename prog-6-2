//output the GCD of two integers
//Euclid algorthm
#include <stdio.h>

int main()
{
    int m,n,i = 0;
    printf("Enter two integers:");
    scanf("%d%d",&m,&n);
    while(n != 0)
    {
        i = m % n;
        m = n;
        n = i;
    }
    printf("Greatest common divisors:%d",m);
    return 0;
}
